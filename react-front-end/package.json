{
  "name": "openequella-react-front-end",
  "private": true,
  "config": {
    "buildlang": "target/resources/lang",
    "devjs": "../Source/Plugins/Core/com.equella.core/target/scala-2.12/classes/web/reactjs",
    "devlang": "../Source/Plugins/Core/com.equella.core/target/scala-2.12/classes/lang",
    "dist": "target/resources/web/reactjs",
    "languageBundle": "target/compile-language-bundle",
    "jest": {
      "verbose": true
    },
    "parcelEntryFiles": "entrypoint/*.html entrypoint/scripts/*.js",
    "tools": "target/tools"
  },
  "engines": {
    "npm": "^7.19.1"
  },
  "scripts": {
    "prepare": "cross-env-shell \"cd ../oeq-ts-rest-api && npm ci build\"",
    "install": "cross-env-shell \"mkdirp ${npm_package_config_devlang} ${npm_package_config_buildlang}\"",
    "clean": "rm -rf target/ node_modules/ .cache/",
    "compile:langbundle-tool": "cross-env-shell tsc --types \"node\" --outDir ${npm_package_config_languageBundle} build-tools/BuildLanguageBundle.ts",
    "build:langbundle-tool": "cross-env-shell parcel build ${npm_package_config_languageBundle}/build-tools/BuildLanguageBundle.js --no-minify --no-source-maps --target node --out-dir=${npm_package_config_tools}",
    "build:langbundle": "cross-env-shell \"node ${npm_package_config_tools}/BuildLanguageBundle.js > ${npm_package_config_buildlang}/jsbundle.json\"",
    "build:bundle": "cross-env-shell parcel build --public-url=. --out-dir=${npm_package_config_dist} ${npm_package_config_parcelEntryFiles}",
    "build": "npm-run-all compile:* build:*",
    "dev": "npm run build && cross-env-shell parcel watch --no-autoinstall --public-url=. --out-dir=${npm_package_config_devjs} ${npm_package_config_parcelEntryFiles}",
    "test": "jest test",
    "storybook": "start-storybook -s ./node_modules,./__stories__/static-files -p 6006",
    "build-storybook": "build-storybook -s ./node_modules,./__stories__/static-files"
  },
  "dependencies": {
    "@date-io/luxon": "1.3.13",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "3.3.10",
    "@openequella/rest-api-client": "file:../oeq-ts-rest-api",
    "@tinymce/tinymce-react": "3.12.6",
    "axios": "0.21.1",
    "clsx": "1.1.1",
    "es6-object-assign": "1.1.0",
    "fp-ts": "2.11.1",
    "fp-ts-contrib": "0.1.26",
    "history": "4.10.1",
    "jspolyfill-array.prototype.find": "0.1.3",
    "lodash": "4.17.21",
    "luxon": "1.28.0",
    "oeq-cloudproviders": "git+https://github.com/apereo/openEQUELLA-cloudprovidersdk.git#32d958ddfff64ca748e7e1b2eae0f0487946a487",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-color": "2.19.3",
    "react-dom": "17.0.2",
    "react-dropzone": "11.3.4",
    "react-html-parser": "2.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-hash-link": "2.4.3",
    "react-visibility-sensor": "3.14.0",
    "runtypes": "6.3.1",
    "shallow-equal-object": "1.1.1",
    "sprintf-js": "1.1.2",
    "timeago-react": "2.0.1",
    "tinymce": "5.9.0",
    "typescript-fsa": "2.5.0",
    "typescript-fsa-reducers": "0.4.5",
    "uuid": "8.3.2",
    "wicked-good-xpath": "1.3.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@storybook/addon-a11y": "6.3.7",
    "@storybook/addon-essentials": "6.3.7",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/react": "6.3.7",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.172",
    "@types/luxon": "2.0.1",
    "@types/node": "16.4.10",
    "@types/react": "17.0.19",
    "@types/react-autosuggest": "10.1.5",
    "@types/react-beautiful-dnd": "13.1.1",
    "@types/react-color": "3.0.5",
    "@types/react-dom": "17.0.9",
    "@types/react-html-parser": "2.0.2",
    "@types/react-router": "5.1.16",
    "@types/react-router-dom": "5.1.8",
    "@types/react-router-hash-link": "2.4.1",
    "@types/react-test-renderer": "17.0.1",
    "@types/rewire": "2.5.28",
    "@types/sprintf-js": "1.1.2",
    "@types/tinymce": "4.6.4",
    "@types/uuid": "8.3.1",
    "babel-jest": "27.0.6",
    "core-js": "3.16.3",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "jest": "27.0.6",
    "jest-config": "27.0.6",
    "mkdirp": "1.0.4",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.5",
    "rewire": "5.0.0",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5"
  }
}
