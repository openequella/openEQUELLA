{
  "name": "openequella-react-front-end",
  "private": true,
  "config": {
    "buildlang": "target/resources/lang",
    "devjs": "../Source/Plugins/Core/com.equella.core/target/scala-2.13/classes/web/reactjs",
    "devlang": "../Source/Plugins/Core/com.equella.core/target/scala-2.13/classes/lang",
    "dist": "target/resources/web/reactjs",
    "languageBundle": "target/compile-language-bundle",
    "jest": {
      "verbose": true
    },
    "parcelEntryFiles": "entrypoint/*.html entrypoint/scripts/*.js",
    "tools": "target/tools"
  },
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.3"
  },
  "scripts": {
    "prepare": "cross-env-shell \"cd ../oeq-ts-rest-api && npm ci && npm run build\"",
    "install": "cross-env-shell \"mkdirp ${npm_package_config_devlang} ${npm_package_config_buildlang}\"",
    "clean": "rm -rf target/ node_modules/ .cache/",
    "compile:langbundle-tool": "cross-env-shell tsc --types \"node\" --outDir ${npm_package_config_languageBundle} build-tools/BuildLanguageBundle.ts",
    "build:langbundle-tool": "cross-env-shell parcel build ${npm_package_config_languageBundle}/build-tools/BuildLanguageBundle.js --no-minify --no-source-maps --target node --out-dir=${npm_package_config_tools}",
    "build:langbundle": "cross-env-shell \"node ${npm_package_config_tools}/BuildLanguageBundle.js > ${npm_package_config_buildlang}/jsbundle.json\"",
    "build:bundle": "cross-env-shell parcel build --public-url=. --out-dir=${npm_package_config_dist} ${npm_package_config_parcelEntryFiles}",
    "build": "npm-run-all compile:* build:*",
    "dev": "npm run build && cross-env-shell parcel watch --no-autoinstall --public-url=. --out-dir=${npm_package_config_devjs} ${npm_package_config_parcelEntryFiles}",
    "test": "jest test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@date-io/luxon": "1.3.13",
    "@fontsource/roboto": "4.5.8",
    "@fontsource/material-icons": "4.5.4",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@material-ui/pickers": "3.3.10",
    "@openequella/rest-api-client": "file:../oeq-ts-rest-api",
    "@tinymce/tinymce-react": "4.2.0",
    "axios": "0.27.2",
    "clsx": "1.2.1",
    "es6-object-assign": "1.1.0",
    "fp-ts": "2.13.1",
    "fp-ts-contrib": "0.1.29",
    "history": "4.10.1",
    "html-react-parser": "3.0.4",
    "jspolyfill-array.prototype.find": "0.1.3",
    "lodash": "4.17.21",
    "luxon": "1.28.0",
    "io-ts": "2.2.19",
    "oeq-cloudproviders": "git+https://github.com/apereo/openEQUELLA-cloudprovidersdk.git#0debbd148bb47d0dfc2270bb47db028e70a22f74",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.1",
    "react-color": "2.19.3",
    "react-dom": "17.0.2",
    "react-dropzone": "14.2.3",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-router-hash-link": "2.4.3",
    "runtypes": "6.6.0",
    "shallow-equal-object": "1.1.1",
    "sprintf-js": "1.1.2",
    "tinymce": "6.2.0",
    "uuid": "8.3.2",
    "wicked-good-xpath": "1.3.0",
    "object-hash": "3.0.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@storybook/addon-a11y": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/react": "6.5.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/jquery": "3.5.14",
    "@types/lodash": "4.14.189",
    "@types/luxon": "2.4.0",
    "@types/node": "16.18.3",
    "@types/react": "17.0.52",
    "@types/object-hash": "2.2.1",
    "@types/react-autosuggest": "10.1.5",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "17.0.18",
    "@types/react-router": "5.1.19",
    "@types/react-router-dom": "5.3.3",
    "@types/react-router-hash-link": "2.4.5",
    "@types/react-test-renderer": "17.0.2",
    "@types/rewire": "2.5.28",
    "@types/sprintf-js": "1.1.2",
    "@types/uuid": "8.3.4",
    "babel-jest": "29.3.1",
    "core-js": "3.26.1",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "jest": "29.3.1",
    "jest-config": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "mkdirp": "1.0.4",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.5",
    "rewire": "6.0.0",
    "ts-jest": "29.0.3",
    "typescript": "4.7.4"
  }
}
