    <description>
      This tag is always used within an enclosing searchBar tag and it defines a selection
      in the drop down of the search bar.
    </description>
    <attribute>
      <name>label</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Label of the option
      </description>
    </attribute>
    <attribute>
      <name>value</name>
