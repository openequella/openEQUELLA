<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.tle.common.customlinks" version="1">
	<attributes>
		<!-- We need to mark this as an Admin Console plug-in because there is no specific Admin Console plug-in for custom links 
			that depends on this. We still need this plug-in for the Security Manager though -->
		<attribute id="type" value="admin-console" />
	</attributes>
	<requires>
		<import plugin-id="com.tle.common.entity" />
	</requires>
	<runtime>
		<library type="code" path="classes/" id="core">
			<export prefix="*" />
		</library>
	</runtime>
</plugin>
