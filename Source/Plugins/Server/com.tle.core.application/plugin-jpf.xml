<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.tle.core.application" version="1">
  <requires>
    <import plugin-id="com.tle.platform.common" />
  </requires>
  <runtime>
    <library type="code" path="classes/" id="code">
      <export prefix="*" />
    </library>
  </runtime>
  <extension-point id="onStartup">
    <parameter-def id="bean" type="string" multiplicity="one" />
    <parameter-def id="type" type="string" multiplicity="one" />
    <parameter-def id="order" type="number" />
  </extension-point>
</plugin>
