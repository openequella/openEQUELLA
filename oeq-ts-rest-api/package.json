{
  "name": "@openequella/rest-api-client",
  "version": "2022.2.0-Alpha",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "22.18.0",
    "npm": "11.4.2"
  },
  "scripts": {
    "prepare": "cd gen-io-ts && npm ci && npm run gen && cd -",
    "start": "rollup --config --watch --bundleConfigAsCjs",
    "build": "rollup --config --bundleConfigAsCjs",
    "test": "jest --no-cache",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "format": "eslint --fix \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "clean": "rm -rf node_modules/ src/gen dist"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "axios": "1.11.0",
    "fp-ts": "2.16.11",
    "io-ts": "2.2.22",
    "io-ts-types": "0.5.19",
    "lodash": "^4.17.21",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "query-string": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/preset-env": "7.28.0",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.16.3",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "axios-cookiejar-support": "6.0.4",
    "core-js": "3.45.0",
    "eslint": "9.34.0",
    "eslint-plugin-unused-imports": "4.2.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-notice": "1.0.0",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "30.0.5",
    "jest-config": "30.0.5",
    "prettier": "3.6.2",
    "rollup": "4.46.2",
    "rollup-plugin-typescript2": "0.36.0",
    "tough-cookie": "6.0.0",
    "ts-jest": "29.4.1",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  }
}
