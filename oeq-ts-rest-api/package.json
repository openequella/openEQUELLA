{
  "name": "@openequella/rest-api-client",
  "version": "2022.2.0-Alpha",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "16.20.2",
    "npm": "8.19.4"
  },
  "scripts": {
    "prepare": "cd gen-io-ts && npm ci && npm run gen && cd -",
    "start": "rollup --config --watch --bundleConfigAsCjs",
    "build": "rollup --config --bundleConfigAsCjs",
    "test": "jest --no-cache",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "format": "eslint --fix \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "clean": "rm -rf node_modules/ src/gen dist"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "axios": "1.3.4",
    "axios-cookiejar-support": "^4.0.0",
    "fp-ts": "2.16.1",
    "io-ts": "2.2.20",
    "io-ts-types": "0.5.19",
    "lodash": "^4.17.21",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "query-string": "^7.0.0",
    "runtypes": "6.6.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.197",
    "@types/node": "18.17.5",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "core-js": "3.32.1",
    "eslint": "8.47.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.6.3",
    "jest-config": "29.6.2",
    "prettier": "2.8.8",
    "rollup": "3.28.0",
    "rollup-plugin-typescript2": "0.35.0",
    "ts-jest": "29.1.1",
    "tslib": "2.6.2",
    "typescript": "4.9.5"
  }
}
